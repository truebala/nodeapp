@model PoojaStores.Models.ModelClasses.CustOrderDetail@{    System.Globalization.CultureInfo Indian = new System.Globalization.CultureInfo("hi-IN");}<div class="container account-container custom-account-container mb-4">    <div class="row">        <div class="col-sm-12 col-6 product-default left-details product-list mb-2">            <figure>                <a href="~/Home/ProductDetails?id=@Model.ProductId">                    <img src="~/ProductImages/@Model.Image" alt="product" width="100" height="100">                    @*<img src="assets/images/products/product-5-1.jpg" alt="product" width="250" height="250">*@                </a>            </figure>            <div class="product-details">                <div class="category-list">                    <a href="~/Home/ProductsList?catDetail=@Model.CategoryId,0" class="product-category">@Model.CategoryName</a>                </div>                <h3 class=""> <a href="~/Home/ProductDetails?id=@Model.ProductId">@Model.ProductName</a> </h3>                <!--<div class="ratings-container">                    <div class="product-ratings">                        <span class="ratings" style="width:100%"></span>-->                <!-- End .ratings -->                <!--<span class="tooltiptext tooltip-top">5.00</span>                </div>-->                <!-- End .product-ratings -->                <!--</div>-->                <!-- End .product-container -->                <p class="product-description">                    @Model.ProductDescription                </p>            <div class="price-box">                <span class="product-price">₹ @(String.Format(Indian,"{0:N}",Model.TotalValue))/-(Including Tax)</span>            </div>            <!-- End .price-box -->            <div class="product-action">                <a href="~/Home/ProductDetails?id=@Model.ProductId" class="btn-icon btn-add-cart">                    <i class="icon-shopping-cart"></i>                    <span>Buy It Again</span>                </a>            </div>        </div>        <!-- End .product-details -->    </div></div><div class="shippingDetails">    <div class="row">        <div class="col-md-4">            <div>                <h4>Shipping Address</h4>                <ul>                    <li>@Model.FullName</li>                    <li>@Model.Address1</li>                    @if (Model.Address2 != null)
                    {                        <li>@Model.Address2</li>                    }                    <li>@Model.CityName, @Model.StateName, @Model.ZipCode</li>                    <li>@Model.CountryName</li>                </ul>            </div>        </div>        <div class="col-md-4">            <div>                <h4> Paymant Status </h4>                <ul>                    <li>@Model.PaymentStatus</li>                    @if (Model.InvoiceDocumentUrl != null)
                    {                        <li><a href="~/ProductImages/@Model.InvoiceDocumentUrl"><div class="btn btn-success btn-sm mt-4"><i class="fa fa-download mr-1"></i>Invoice Download</div></a></li>                    }                </ul>            </div>        </div>        <div class="col-md-4">            <h4> Order Summary </h4>            <ul>                <li>Item(s) Subtotal: <strong><span class="fa-pull-right">@(String.Format(Indian,"{0:N}",Model.UnitPrice))/-</span></strong></li>                <li>GST: <strong><span class="fa-pull-right">@(String.Format(Indian,"{0:N}",Model.GST))/-</span></strong></li>                @*<li>Total: <strong><span class="fa-pull-right">24,999.00</span></strong></li>*@                <li class="text-black"><strong>Grand Total:<span class="fa-pull-right">@(String.Format(Indian,"{0:N}",Model.TotalValue))/-</span></strong></li>            </ul>        </div>    </div></div></div>