<script>
    function WishList(id) {
        var text = $("#sale").text();
        if (text == "ADD TO WISHLIST") {
            $.ajax({
                url: GlobalUrl + "Sales/SaveWishList?id=" + id,
                type: 'post',
                data: '{}',
                success: function (result) {
                    if (result.result.statusCode == 1) {
                        toastr["success"]("Successuflly Added!");
                        window.location = "ProductDetails?id=" + id;
                    }
                    else if (result.result.statusCode == 5) {
                        window.location = "GoToLogin";
                        toastr["error"]("Added cancelled!")
                    }
                    else {
                        toastr["error"](result.result.statusMessage)
                    }
                }
            });
        } else {
            window.location = "GoToWishList";
        }
    }
    function cart(id) {
        var text = $("#carts").text();
        if (text == "  Add to Cart") {
            var cnt = $("#prodQuantity").val();
            $.ajax({
                url: GlobalUrl + "Sales/SaveCart",
                type: 'post',
                data: {
                    'id': id,
                    'qty': cnt,
                },
                success: function (result) {
                    if (result.result.statusCode == 1) {
                        toastr["success"]("Sucessfully Added!");
                        //$("#carts").hide();
                        //$("#alter").hide();
                        //$("#view").show();
                        window.location.href = window.location.href;
                    }
                    else if (result.result.statusCode == 5) {
                        window.location = "GoToLogin";
                    } else if (result.result.statusCode == 2) {
                        toastr["error"]("Exceeding Available Quantity")
                    }
                    else {
                        toastr["error"](result.result.statusMessage)
                    }
                }
            });
        }
    }
    function cartview() {
        var Yes = "Yes";
        var No = "No";
        var text =@ViewBag.cart;
        if (text == "Yes") {
            $("#carts").html("<i class='fas fa-shopping-bag'></i>  Add to Cart");
            $("#view").hide();
        } else {
            $("#alter").hide();
            $("#carts").hide();
        }
    }
    $(document).ready(function () {
        cartview();
    });
</script>