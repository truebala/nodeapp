@model PoojaStores.Models.ModelClasses.ProductMasterModel
@{
    System.Globalization.CultureInfo Indian = new System.Globalization.CultureInfo("hi-IN");
}
<main class="main">
    <div class="container">
        <nav aria-label="breadcrumb" class="breadcrumb-nav">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="~/Home/Index"><i class="icon-home"></i></a></li>
                <li class="breadcrumb-item"><a href="javascript:void(0)">@Model.ProductTitle</a></li>
            </ol>
        </nav>

        <div class="product-single-container product-single-default">
            <div class="cart-message d-none">
                <strong class="single-cart-notice">“Men Black Sports Shoes”</strong>
                <span>has been added to your cart.</span>
            </div>

            <div class="row">
                <div class="col-lg-5 col-md-6 product-single-gallery">
                    <div class="product-slider-container">
                        <div class="label-group">
                            @if (Model.DiscountPercentage != 0)
                            {
                                <div class="product-label label-hot">HOT</div>

                                <div class="product-label label-sale">
                                    -@Model.DiscountPercentage %
                                </div>
                            }
                        </div>
                        <div class="product-single-carousel owl-carousel owl-theme show-nav-hover">
                            <div class="product-item">
                                <img class="product-single-image" src="~/ProductImages/@Model.ProductMainImage1" data-zoom-image="../ProductImages/@Model.ProductMainImage1" width="468" height="468" alt="product" />
                            </div>
                            @if (Model.ProductMainImage2 != null)
                            {
                                <div class="product-item">
                                    <img class="product-single-image" src="~/ProductImages/@Model.ProductMainImage2" data-zoom-image="../ProductImages/@Model.ProductMainImage2" width="468" height="468" alt="product" />
                                </div>
                            }
                            @foreach (var o in Model.OtherImages)
                            {
                                <div class="product-item">
                                    <img class="product-single-image" src="~/ProductImages/@o" data-zoom-image="../ProductImages/@o" width="468" height="468" alt="product" />
                                </div>
                            }
                        </div>
                        <!-- End .product-single-carousel -->
                        <span class="prod-full-screen">
                            <i class="icon-plus"></i>
                        </span>
                    </div>

                    <div class="prod-thumbnail owl-dots">
                        <div class="owl-dot">
                            <img src="~/ProductImages/@Model.ProductMainImage1" alt="product-thumbnail" />
                        </div>
                        <div class="owl-dot">
                            <img src="~/ProductImages/@Model.ProductMainImage2" alt="product-thumbnail" />
                        </div>
                        @foreach (var o in Model.OtherImages)
                        {
                            <div class="owl-dot">
                                <img src="~/ProductImages/@o" alt="product-thumbnail" />
                            </div>
                        }
                    </div>
                </div>
                <!-- End .product-single-gallery -->

                <div class="col-lg-7 col-md-6 product-single-details">
                    <h1 class="product-title">@Model.ProductTitle</h1>
                    <!--<div class="ratings-container">
                        <div class="product-ratings">
                            <span class="ratings" style="width:60%"></span>-->
                    <!-- End .ratings -->
                    <!--<span class="tooltiptext tooltip-top"></span>
                    </div>-->
                    <!-- End .product-ratings -->
                    <!--<a href="#" class="rating-link">( 6 Reviews )</a>
                    </div>-->
                    <!-- End .ratings-container -->

                    <hr class="short-divider">

                    <div class="price-box">
                        @if (Model.DiscountPercentage != 0)
                        {
                            <span class="old-price">Rs. @(String.Format(Indian,"{0:N}",Model.SellingPrice))</span>
                        }
                        <span class="new-price">Rs.  @(String.Format(Indian,"{0:N}",Model.DiscountedCost))</span>
                    </div>
                    <!-- End .price-box -->

                    <div class="product-desc">
                        <p>
                            @Model.ProductDescription
                        </p>
                    </div>
                    <!-- End .product-desc -->

                    <ul class="single-info-list">

                        <li>
                            SQUId: <strong>@Model.SQUID</strong>
                        </li>

                        <li>
                            CATEGORY: <strong><a href="~/Home/ProductsList?catDetail=@Model.CategoryId,0" class="product-category">@Model.CategoryName</a></strong>
                        </li>
                        <li>
                            Quantity Available : <strong>@Model.QtyAvailable</strong>
                        </li>
                        <li>
                            Minimum Order Quantity : <strong>@Model.MinimumOrderQty</strong>
                        </li>

                        @*<li>
                                TAGs: <strong><a href="#" class="product-category">POOJA</a></strong>,
                                <strong><a href="#" class="product-category">VRATHAM</a></strong>
                            </li>*@
                    </ul>

                    <div class="product-action">
                        <div class="product-single-qty" id="alter">
                            <input class="horizontal-quantity form-control" type="text" id="prodQuantity" value="@Model.MinimumOrderQty">
                        </div>
                        <!-- End .product-single-qty -->

                        <a href="javascript:;" class="btn btn-dark " title="Add to Cart" id="carts" onclick="cart(@Model.ProductId)">Add to Cart</a>

                        <a href="~/Sales/Cart" class="btn btn-gray" id="view">View cart</a>
                    </div>
                    <!-- End .product-action -->

                    <hr class="divider mb-0 mt-0">

                    <div class="product-single-share mb-3">
                        <label class="sr-only">Share:</label>

                        @*<div class="social-icons mr-2">
                                <a href="#" class="social-icon social-facebook icon-facebook" target="_blank" title="Facebook"></a>
                                <a href="#" class="social-icon social-twitter icon-twitter" target="_blank" title="Twitter"></a>
                                <a href="#" class="social-icon social-linkedin fab fa-linkedin-in" target="_blank" title="Linkedin"></a>
                                <a href="#" class="social-icon social-gplus fab fa-google-plus-g" target="_blank" title="Google +"></a>
                                <a href="#" class="social-icon social-mail icon-mail-alt" target="_blank" title="Mail"></a>
                            </div>*@
                        <!-- End .social-icons -->
                        @if (ViewBag.test == true)
                        {
                            <a href="~/Sales/WishList" title="Add to Wishlist">
                                @*❤️*@ <div class="d-xl-heart"><img src="~/assets/images/heart.png" /><span>&nbsp; BROWSE   WISHLIST</span></div>

                            </a>
                        }
                        else
                        {
                            <a href="#" title="Add to Wishlist" onclick="WishList(@Model.ProductId)">
                                <i class="icon-wishlist-2"></i><span id="sale">ADD TO WISHLIST</span>
                            </a>
                        }
                    </div>
                    <!-- End .product single-share -->
                </div>
                <!-- End .product-single-details -->
            </div>
            <!-- End .row -->
        </div>
        <!-- End .product-single-container -->
