@{ 
    System.Globalization.CultureInfo Indian = new System.Globalization.CultureInfo("hi-IN");
}
<main class="main home">
    <div class="container mt-2">
        <div class="row">
            <div class="col-lg-3 mb-2 appear-animate" data-animation-name="fadeInRightShorter" data-animation-delay="200" data-animation-duration="1000">
                <div class="side-menu-wrapper">
                    <h2 class="side-menu-title ls-10">Top Categories</h2>

                    <div class="side-menu-body mb-2 px-3 mx-3">
                        <ul class="side-menu pb-1">
                           @foreach (var c in ViewBag.HeadCats)
                            {
                                <li>
                                    <a href="~/Home/ProductsList?catDetail=@c.CategoryId,0">
                                        <img src="~/ProductImages/@c.CategoryImage" width="25px" height="25px" alt="pooja" class="mr-2">
                                            @c.CategoryName
                                    </a>
                                </li>
                            }
                        </ul>

                        @*<a href="javascript:void(0)" class="btn btn-block btn-dark btn-lg px-0 ls-10">
                            Huge Sale -
                            <strong>70%</strong>
                            Off
                        </a>*@
                    </div>
                    <!-- End .side-menu-body -->
                </div>
                <!-- End .side-custom-menu -->
            </div>
            <!-- End .col-lg-3 -->

            <div class="col-lg-9 mb-2 appear-animate" data-animation-name="fadeInLeftShorter" data-animation-delay="400" data-animation-duration="1000">
                <div class="home-slider owl-carousel owl-theme" data-owl-options="{
							'dots': true,
							'nav': false
						}">
                    @foreach (var b in ViewBag.banners)
                    {
                    <div class="home-slide home-slide1 banner banner-md-vw banner-sm-vw d-flex align-items-center">
                        <img class="slide-bg" style="background-color: #71b5da;" src="~/ProductImages/@b.Image" width="835" height="415" alt="slider image">
                        <div class="banner-layer">
                            <h4 class="text-white">@b.ImageTitle1</h4>
                            <h2 class="text-white mb-0">@b.ImageTitle2</h2>
                            <h3 class="text-white text-uppercase">@b.DiscountPercent% Off</h3>
                            <h5 class="text-white text-uppercase d-inline-flex ls-n-20 mb-0">
                                Starting&nbsp At <b class="coupon-sale-text text-secondary bg-white">
                                ₹<em class="align-text-top">@b.StartingAt</em>
                               </b>
                            </h5>
                            <div>
                                <a href="~/Home/ProductsList?catDetail=@b.RelatedCategoryId,0" class="btn btn-danger btn-red">@b.TextOnButton</a>
                            </div>
                            
                        </div>
                        <!-- End .banner-layer -->
                    </div>
                    }
                    <!-- End .home-slide -->

                  
                </div>
                <!-- End .home-slider -->
            </div>
            <!-- End .col-lg-9 -->
        </div>
        <!-- End .row -->


